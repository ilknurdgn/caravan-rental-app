}},{"generated":{"line":110,"column":9},"source":"components/examples/input/code-explore.js","original":{"line":33,"column":6}},{"generated":{"line":111,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":46,"column":8}},{"generated":{"line":112,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":46,"column":8}},{"generated":{"line":113,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":46,"column":8}},{"name":"elements","generated":{"line":114,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":46,"column":13}},{"generated":{"line":115,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":46,"column":8}},{"generated":{"line":116,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":33,"column":6}},{"generated":{"line":116,"column":7},"source":"components/examples/input/code-explore.js","original":{"line":32,"column":4}},{"generated":{"line":117,"column":0},"source":"components/examples/input/code-explore.js","original":{"line":49,"column":3}},{"name":"React","generated":{"line":121,"column":2},"source":"components/examples/input/code-explore.js","original":{"line":11,"column":41}},{"name":"Component","generated":{"line":121,"column":18},"source":"components/examples/input/code-explore.js","original":{"line":11,"column":47}},{"name":"CodeExplore","generated":{"line":123,"column":18},"source":"components/examples/input/code-explore.js","original":{"line":11,"column":21}}],"sources":{"components/examples/input/code-explore.js":"/* @flow */\n\nimport React from 'react';\nimport ExampleBox from '../ExampleBox';\n\ntype code = {| code: string, value: string |};\ntype State = {\n  codes: code[],\n};\n\nexport default class CodeExplore extends React.Component<{||}, State> {\n  state: State = { codes: [] };\n\n  render() {\n    const { codes } = this.state;\n    const undefCodes: Array<code | void> = [undefined, undefined, undefined];\n    const elements = undefCodes\n      .concat(codes)\n      .reverse()\n      .slice(0, 3)\n      .map((code, i) => {\n        if (code === undefined) {\n          return <li key={i}>...</li>;\n        }\n        return (\n          <li key={i}>\n            {code.code} =&gt; {code.value}\n          </li>\n        );\n      });\n\n    return (\n      <ExampleBox>\n        <h2>\n          Input&nbsp;\n          <code>KeyboardEvent</code>\n          &nbsp;\n          <code>.code</code>\n          &nbsp; and <code>.value</code>\n          &nbsp; explore:\n        </h2>\n\n        <input onKeyPress={this.handleKeyPress} />\n\n        <p>Last 3 values (code =&gt; value):</p>\n        <ol>{elements}</ol>\n      </ExampleBox>\n    );\n  }\n\n  handleKeyPress = (e: SyntheticKeyboardEvent<HTMLInputElement>) => {\n    const { codes } = this.state;\n\n    codes.push({\n      // $FlowFixMe\n      code: e.nativeEvent.code,\n      value: e.key,\n    });\n    if (codes.length > 3) {\n      codes.shift();\n    }\n\n    this.setState({ codes });\n  };\n}\n"},"lineCount":null}},"hash":"6813bc28fb3bac5f243f1eb945da11c0","cacheData":{"env":{}}}